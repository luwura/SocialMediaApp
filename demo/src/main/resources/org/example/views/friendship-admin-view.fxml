<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.FriendshipAdminController">
    <children>
        <VBox layoutX="15.0" layoutY="23.0" prefHeight="355.0" prefWidth="570.0">
            <children>
                <TableView fx:id="tableView" prefHeight="291.0" prefWidth="566.0">
                    <columns>
                        <TableColumn fx:id="tableColumnUser1" prefWidth="131.0" text="Id1" />
                        <TableColumn fx:id="tableColumnUser2" prefWidth="148.0" text="Id2" />
                        <TableColumn fx:id="tableColumnDate" prefWidth="290.0" text="Date" />
                    </columns>
                </TableView>
                <ButtonBar prefHeight="59.0" prefWidth="532.0">
                    <buttons>
                        <Button mnemonicParsing="false" onAction="#handleAddFriendship" text="Add Friendship">
                            <font>
                                <Font name="Candara Light" size="14.0" />
                            </font>
                        </Button>
                        <Button mnemonicParsing="false" onAction="#handleDeleteFriendship" text="Delete Friendship">
                            <font>
                                <Font name="Candara Light" size="14.0" />
                            </font>
                        </Button>
                        <Button mnemonicParsing="false" text="Exit">
                            <font>
                                <Font name="Candara Light" size="14.0" />
                            </font>
                        </Button>
                    </buttons>
                    <VBox.margin>
                        <Insets right="50.0" />
                    </VBox.margin>
                </ButtonBar>
            </children>
        </VBox>
    </children>
</AnchorPane>
